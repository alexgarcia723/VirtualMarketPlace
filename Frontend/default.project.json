{
  "name": "CEN4010_Project_Frontend",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      },
      "Remotes": {
        "$className": "Folder",
        "PlaceOrder": {
          "$className": "RemoteFunction"
        },
        "FillOrder": {
          "$className": "RemoteFunction"
        },
        "MarketFillOrder": {
          "$className": "RemoteFunction"
        },
        "RequestCancelOrder": {
          "$className": "RemoteFunction"
        },         
        "GetNumOrdersPerPage": {
          "$className": "RemoteFunction"
        },   
        "UpdateClientItemOrderHistories": {
          "$className": "RemoteEvent"
        },
        "PlayerDataUpdate": {
          "$className": "RemoteEvent"
        },
        "UpdateServerViewingPages": {
          "$className": "RemoteEvent"
        },
        "UpdateClientPages": {
          "$className": "RemoteEvent"
        }
      }
    },

    "HttpService": {
      "$properties": {
        "HttpEnabled": true
      }
    },

    "Players": {
      "$properties": {
        "CharacterAutoLoads": false
      }
    },

    "ServerScriptService": {
      "$path": "src/server/"
    },

    "StarterGui": {
      "MarketplaceUI": {
        "$path": "src/assets/MarketplaceUI.rbxm"
      }, "TopbarUI": {
        "$path": "src/assets/TopbarUI.rbxm"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
          "$path": "src/client"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel",
        "ClockTime": 0
      },
       "BlankBackground": {
        "$className": "Sky",
        "$properties": {
          "CelestialBodiesShown": false
        }
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    },
    "TextChatService": {
      "$properties": {
        "ChatVersion": "TextChatService"
      }
    }
  }
}